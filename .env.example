# Firebase Configuration for Web
# Copy this file to.env and fill in your Firebase project details

# Firebase Web Config
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
FIREBASE_DATABASE_URL=https://your-project-id-default-rtdb.asia-southeast1.firebasedatabase.app/
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Optional: Analytics Measurement ID
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# App Configuration
APP_NAME=Wait A Minute
APP_VERSION=1.0.0

# Environment
NODE_ENV=production